   "source": [
    "def m_u_test(data_1, data_2):\n",
    "    stat, pvalue = mannwhitneyu (data_1, data_2, alternative='two-sided')\n",
    "    alpha = 0.05\n",
    "    return pvalue"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "metadata": {},
   "outputs": [],
   "source": [
    "same = []\n",
    "not_same = []\n",
    "alpha = 0.05\n",
    "for col in test.columns[1:]:\n",
    "    if m_u_test(test[col], train[col]) < alpha:\n",
    "        same.append(col)\n",
    "    else:\n",
    "        not_same.append(col)"
   ]
